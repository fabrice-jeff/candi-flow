{% extends 'base.html.twig' %}

{% block title %}Domaine{% endblock %}

{% block body %}
    <h1>Domaine</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ domaine.id }}</td>
            </tr>
            <tr>
                <th>Libelle</th>
                <td>{{ domaine.libelle }}</td>
            </tr>
            <tr>
                <th>Code</th>
                <td>{{ domaine.code }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ domaine.createdAt ? domaine.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ domaine.updatedAt ? domaine.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Deleted</th>
                <td>{{ domaine.deleted ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_domaine_index') }}">back to list</a>

    <a href="{{ path('app_domaine_edit', {'id': domaine.id}) }}">edit</a>

    {{ include('domaine/_delete_form.html.twig') }}
{% endblock %}
